{
  "short_name": "MyTweets",
  "name": "Tweets Listener",
  "start_url": "/",
  "theme_color": "#3c97ed",
  "background_color": "#ffffff",
  "display": "standalone",
  "orientation": "portrait",
  "icons": [
    {
      "src": "/assets/images/icons/favicon-16x16.png",
      "sizes": "16x16",
      "type": "image/png"
    },
    {
      "src": "/assets/images/icons/favicon-32x32.png",
      "sizes": "32x32",
      "type": "image/png"
    },
    {
      "src": "/assets/images/icons/apple-touch-icon-72x72-precomposed.png",
      "sizes": "72x72",
      "type": "image/png"
    },
    {
      "src": "/assets/images/icons/apple-touch-icon-120x120.png",
      "sizes": "120x120",
      "type": "image/png"
    },
    {
      "src": "/assets/images/icons/android-chrome-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/assets/images/icons/android-chrome-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "dataGroups": [
    {
      "name": "oauth-api",
      "urls": [
        "https://api.twitter.com/**",
        "https://oauth.io/auth/twitter/me/**",
        "https://oauth.io/auth/twitter/**",
        "https://oauth.io/api/extended-endpoints/**",
        "https://api.twitter.com/1.1/statuses/user_timeline.json",
        "https://oauth.io/request/twitter/https%3A%2F%2Fapi.twitter.com%2F1.1%2Fstatuses%2Fuser_timeline.json",
        "http://pbs.twimg.com/**"
      ],
      "cacheConfig": {
        "maxSize": 1000,
        "maxAge": "3d",
        "timeout": "1m",
        "strategy": "freshness"
      }
    }
  ],

  "assetGroups": [
    {
      "name": "fonts",
      "resources": {
        "urls": [
          "https://fonts.googleapis.com/**",
          "https://fonts.gstatic.com/**",
          "http://pbs.twimg.com/**"
        ]
      }
    }
  ]
}